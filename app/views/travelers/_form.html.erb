<%= form_for(@traveler) do |f| %>
  <%= f.error_messages %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name, :required => true %>
  &nbsp;
  <%= f.label :last_name %>
  <%= f.text_field :last_name, :required => true %><br />

  <%= f.label :email %>
  <%= f.text_field :email, :required => true %>

  <%= f.submit "Submit" %>
<% end %>
