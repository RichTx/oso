<%= link_to "Create User Account", new_admin_user_path %> <br />
<br />
<h2>Users</h2>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to "#{user.email}", [:admin, user] %></li>
      <li><%= user.email %></li><br />
    <% end %>
  </ul>
  <h2>Open Stories: <%= @project.name %> Project</h2>
  <table border="1" cellpadding="5">
    <tr>
      <td>Name</td><td>Type</td>
    </tr>
    <% @stories.each do |s| %>
      <tr>
        <td><%= s::name %></td><td><%= s::story_type %></td>
      </tr>
    <% end %>
  </table>

