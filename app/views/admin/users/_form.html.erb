<%= form_for [:admin, @user] do |f| %>
  <%= f.error_messages %>
    <br>
  <%= f.label :name %>
  <%= f.text_field :name %>
    <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
    <br>
  <%= f.label :password %>
  <%= f.text_field :password %>
    <br>
  <%= f.submit "Submit" %>
<% end %>
